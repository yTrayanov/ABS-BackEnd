  dynamoTablesInit:
    command: 'aws dynamodb create-table --table-name ABS-Table / --attribute-definitions
      / AttributeName=PK,AttributeType=S / AttributeName=SK,AttributeType=S / --key-schema
      / AttributeName=PK,KeyType=HASH / AttributeName=SK,KeyType=RANGE / --global-secondary-indexes
      / [ / "Create": { / "IndexName": "GSI1", / "KeySchema": [{"AttributeName":"PK","KeyType":"HASH"}],
      / "Projection":{ / "ProjectionType":"ALL" / }, / "Create": { / "IndexName":
      "GSI2", / "KeySchema": [{"AttributeName":"PK","KeyType":"HASH"}], / "Projection":{
      / "ProjectionType":"ALL" / } / ] / --provisioned-throughput ReadCapacityUnits=5,WriteCapacityUnits=5
      / --endpoint-url http://host.docker.internal:8000'
    image: garland/aws-cli-docker
    links:
    - dynamodb-local
  dynamodb-local:
    command: -jar DynamoDBLocal.jar -sharedDb -dbPath ./data
    container_name: dynamodb-local
    image: amazon/dynamodb-local:latest
    ports:
    - published: 8000
      target: 8000
    - target: 8000
    volumes:
    - C:\Users\Yavor Trayanov\source\repos\ABS-BackEnd\docker\dynamodb:/home/dynamodblocal/data:rw
    working_dir: /home/dynamodblocal
version: '3.4'