services:
  abs-auth:
    build:
      context: C:\Users\Yavor Trayanov\source\repos\ABS-BackEnd
      dockerfile: ABS-Auth/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: ABS-Auth
      target: base
    container_name: ABS-Auth
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: absauth:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/ABS-Auth.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    tty: true
    volumes:
    - C:\Users\Yavor Trayanov\source\repos\ABS-BackEnd\ABS-Auth:/app:rw
    - C:\Users\Yavor Trayanov\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Yavor Trayanov\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Yavor Trayanov\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\Yavor Trayanov\.nuget\packages:/root/.nuget/packages:ro
